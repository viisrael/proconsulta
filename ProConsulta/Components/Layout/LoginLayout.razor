@using Microsoft.AspNetCore.Identity
@inherits LayoutComponentBase

<MudThemeProvider  IsDarkMode="true"/>
<MudPopoverProvider />
<MudDialogProvider />
<MudSnackbarProvider />

<MudLayout>
    <MudAppBar Elevation="1">
           <MudText Typo="Typo.h6" Class="ml-3">Pro Consulta</MudText> 
           <MudSpacer/>

           <MudStack>
               <MudIconButton Href="/" Icon="@Icons.Material.Filled.Home" Color="Color.Inherit"/>
           </MudStack>
    </MudAppBar>



    <MudMainContent Class="mt-16 pa-4">
        <MudGrid Justify="Justify.Center" AlignItems="AlignItems.Center" Style="min-height: 80vh;">
            <MudItem xs="12" sm="8" md="4">
                @Body
            </MudItem>
        </MudGrid>
    </MudMainContent>
</MudLayout>